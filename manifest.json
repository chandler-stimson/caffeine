{
  "manifest_version": 3,
  "version": "0.2.1",
  "name": "Caffeine - Keep Awake",
  "description": "Keep system or display awake by overriding power-saving settings, which prevents dimming of the screen, or starting a screen saver",
  "permissions": [
    "storage",
    "power",
    "contextMenus",
    "alarms"
  ],
  "optional_permissions": [
    "downloads"
  ],
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "256": "data/icons/256.png"
  },
  "background": {
    "service_worker": "worker.js"
  },
  "action": {},
  "homepage_url": "https://add0n.com/caffeine.html",
  "commands": {
    "_execute_action": {
      "description": "Press the action button"
    }
  }
}
